import math
a=int(input('Введите число a:'))
b=int(input('Введите число b:'))
c=int(input('Введите число c:'))
D=b**2-4*a*c
print(f'D=',D)
if D>0:
    x1=((-b-math.sqrt(D))/(2*a))
    print(f'x1=',x1)
    x2=((-b+math.sqrt(D))/(2*a))
    print(f'x2=',x2)
elif D==0:
    x3=(-b/(2*a))
    print(f'x3=',x3)
else:
    print('not ssolvetion')
